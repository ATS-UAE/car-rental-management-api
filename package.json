{
	"name": "car-rental-management-api",
	"version": "0.2.58",
	"description": "API wrapper for car-rental-management-server.",
	"main": "dist/index.js",
	"scripts": {
		"test:run": "jest --config jest.config.json",
		"test:watch": "jest --watch --config jest.config.json",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src --color --config .eslintrc",
		"build": "rm -rf ./dist/* && tsc",
		"prepare": "npm run build",
		"prepublishOnly": "npm run test:run && npm run lint",
		"preversion": "npm run lint && npm run format",
		"postversion": "git push && git push --tags"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ATS-UAE/car-rental-management-api.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ATS-UAE/car-rental-management-api/issues"
	},
	"homepage": "https://github.com/ATS-UAE/car-rental-management-api#readme",
	"devDependencies": {
		"@types/jest": "^25.2.1",
		"@types/moxios": "^0.4.9",
		"@typescript-eslint/eslint-plugin": "^2.29.0",
		"@typescript-eslint/parser": "^2.29.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-typescript": "^7.2.1",
		"eslint-config-prettier": "^6.11.0",
		"eslint-import-resolver-typescript": "^2.0.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-prettier": "^3.1.3",
		"jest": "^25.4.0",
		"moxios": "^0.4.0",
		"prettier": "^2.0.5",
		"prettier-eslint": "^9.0.1",
		"prettier-eslint-cli": "^5.0.0",
		"ts-jest": "^25.4.0",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"car-rental-management-shared": "git+https://github.com/ATS-UAE/car-rental-management-shared.git",
		"moment": "^2.25.3"
	}
}
